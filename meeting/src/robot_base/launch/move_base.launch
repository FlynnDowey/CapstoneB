<launch>

   <master auto="start"/>
 <!-- Run the map server --> 
   <node name="map_server" pkg="map_server" type="map_server" args="$(find robot_base)/maps/mymap.yaml"/>

 <!--- Run AMCL --> 
   <include file="$(find amcl)/examples/amcl_omni.launch" />   

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
        <rosparam file="$(find robot_base)/newMoveBase/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
        <rosparam file="$(find robot_base)/newMoveBase/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find robot_base)/newMoveBase/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find robot_base)/newMoveBase/global_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find robot_base)/newMoveBase/move_base_params.yaml" command="load" />
        <rosparam file="$(find robot_base)/newMoveBase/base_local_planner_params.yaml" command="load" />
    </node>

</launch> 
